{
  "name": "file-size-inator",
  "displayName": "File Size Inator",
  "description": "Shows file size in the status bar",
  "version": "1.0.0",
  "publisher": "Joan (YoanSinH) Cifuentes",
  "engines": {
    "vscode": "^1.50.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yoansinh/file-size-inator"
  },
  "keywords": [
    "file-size",
    "filesize",
    "file",
    "size",
    "status bar"
  ],
  "categories": [
    "Other"
  ],
  "icon": "icon.jpg",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "File Size Inator",
      "properties": {
        "file-size-inator.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable/disable file size display"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.50.0",
    "@types/node": "^14.x.x",
    "@typescript-eslint/eslint-plugin": "^4.x.x",
    "@typescript-eslint/parser": "^4.x.x",
    "eslint": "^7.x.x",
    "typescript": "^4.x.x",
    "@vscode/test-electron": "^2.3.8",
    "mocha": "^10.2.0",
    "@types/mocha": "^10.0.1"
  }
}
